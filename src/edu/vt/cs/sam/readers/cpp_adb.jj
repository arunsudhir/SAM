  /**
   * Reads a fully qualified name (since it is used during lookahead, we
   * cannot use token. We have to explicitly use getToken).
   */
  static String GetFullyScopedName() /*throws ParseException*/
  {
     Token t = getToken(1);

     if (t.kind != ID && t.kind != SCOPE)
        return null;

     StringBuffer s = new StringBuffer();

     int i;
     if (t.kind != SCOPE)
     {
        s.append(t.image);
        t = getToken(2);
        i = 3;
     }
     else
        i = 2;

     while (t.kind == SCOPE)
     {
        s.append(t.image);
        s.append((t = getToken(i++)).image);
        t = getToken(i++);
     }

     return s.toString();
  }

  public static void endLine(String type, int beginLine, int beginCol, int endLine, int endCol) {
     String text = getText(beginLine, beginCol, endLine, endCol);
     JavaLineAttributes attrs = new JavaLineAttributes();
     attrs.setLineType(type);
     attrs.setLineText(text);
     attrs.setLineNumber(beginLine);
     handler.endLine(attrs);
  }

  protected static void initializeLineEndings() throws IOException {
     int lineEndingLength = getLineEndingLength();
     BufferedReader in = new BufferedReader(new CharArrayReader(bufferedInput));
     int offset = 0;
     lineEndings.add(offset);
     
 
     for(String str = in.readLine(); null != str; str = in.readLine()) {
        offset += str.length() + lineEndingLength;
        lineEndings.add(offset);
     }
     in.close();
  }

  protected static int getLineEndingLength() {
     try {
        BufferedReader in = new BufferedReader(new CharArrayReader(bufferedInput));
        String line = in.readLine();
        int lineLength = line.length();
        if(bufferedInput[lineLength] == '\r') {
           return 2;
        } else {
           return 1;
        }
     } catch(IOException e) {
        throw new RuntimeException("Error getting line ending length: " + e.getMessage(), e);
     }
  }

  protected static String getText(int beginLine, int beginColumn, int endLine, int endColumn) {
     beginColumn = Math.max(1, beginColumn);
     endColumn = Math.max(1, endColumn);
     StringBuilder retVal = new StringBuilder();
     try {
        BufferedReader in = new BufferedReader(new CharArrayReader(bufferedInput));
        int lineEndingLength = getLineEndingLength();
        int offset = lineEndings.size() > 0 ? lineEndings.get(Math.min(lineEndings.size(), beginLine)-1) : 0;
        int lineNum = Math.min(lineEndings.size(), beginLine);
        in.skip(offset);
        for(; lineNum < beginLine; ++lineNum) {
           String line = in.readLine();
           offset += line.length() + lineEndingLength;
           lineEndings.add(offset);
        } 

        while(lineNum < endLine) {
           String inputLine = in.readLine();
           offset += inputLine.length() + lineEndingLength;
           lineEndings.add(offset);
           String toAppend = "";
           if(beginColumn >= 1 && inputLine.length() >= beginColumn) {
              try { toAppend = inputLine.substring(beginColumn - 1, inputLine.length()); }
              catch(StringIndexOutOfBoundsException ex) {
                 System.err.println("Failed getting substring from " + (beginColumn-1) + " through " +
                                    inputLine.length());
              }
           }
           retVal.append(toAppend).append("\n");
           ++lineNum;
           beginColumn = 1;
        }
        String inputLine = in.readLine();
        offset += inputLine.length() + lineEndingLength;
        lineEndings.add(offset);
        endColumn = Math.min(endColumn, inputLine.length());
        try { retVal.append(inputLine.substring(beginColumn-1, endColumn).trim()); }
        catch( StringIndexOutOfBoundsException ex) {
           throw new RuntimeException("begin " + beginLine + ":" + beginColumn + "\tend " +
                                      endLine + ":" + endColumn);
        }
        in.close();
     } catch(IOException e) {
        throw new RuntimeException("Error reading input file: " + e.getMessage(), e);
     }
     return retVal.toString();
  }

  public static void endLine(Node n) {
     MarkedNode mn = (MarkedNode) n;
     int beginLine = mn.beginLine;
     int endLine = mn.endLine;
     int beginCol = mn.beginColumn;
     int endCol = mn.endColumn;
     if(beginLine == endLine && beginCol == endCol) {
        return;
     }
     String text = getText(beginLine, beginCol, endLine, endCol);
     JavaLineAttributes attrs = new JavaLineAttributes();
     attrs.setLineNumber(mn.beginLine);
     attrs.setLineText(text);
     attrs.setLineType(((SimpleNode) n).getNodeName());
     handler.endLine(attrs);
  }

  public static char[] bufferFile(File inputFile) throws IOException {
     CharArrayWriter out = new CharArrayWriter();
     BufferedReader in = new BufferedReader(new FileReader(inputFile));
     char[] buff = new char[4096];
     int numRead = -1;
     while(-1 != (numRead = in.read(buff))) {
        out.write(buff, 0, numRead);
     }
     in.close();
     return out.toCharArray();
  }

